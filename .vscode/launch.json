    "version": "0.2.0",
    "configurations": [
        {
            "name": "Console output",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/seasonality.py",
            "console": "integratedTerminal",
            "args": ["--view", "console"],
            "justMyCode": false
        },
        {
            "name": "PDF",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/seasonality.py",
            "console": "integratedTerminal",
            "args": ["--view", "pdf", "--file", "myplots.pdf"],
            "justMyCode": false
        },
        {
            "name": "PDF annual seasonality",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/seasonality.py",
            "console": "integratedTerminal",
            "args": ["--view", "pdf", "--file", "myplots.pdf", "-nop", "-not", "-nor", "-noas", "-noar", "-noaw", "-noam", "-noaq", "-now", "-ph", "100"],
            "justMyCode": false
        },
        {
            "name": "Help message",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/seasonality.py",
            "console": "integratedTerminal",
            "args": ["--help"],
            "justMyCode": false
        },
        {
            "name": "PDF /w confidence intervals",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/seasonality.py",
            "console": "integratedTerminal",
            "args": ["--view", "pdf", "--file", "myplots.pdf", "-a"],
            "justMyCode": false
        },
   ]
